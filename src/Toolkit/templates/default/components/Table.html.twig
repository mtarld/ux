{%- props -%}
{%- set style = html_cva(
    base: 'relative w-full overflow-auto',
    variants: {},
    compoundVariants: []
) -%}

<table
    class="{{ style.apply({}, attributes.render('class'))|tailwind_merge }}"
    {{ attributes.defaults({}).without('class') }}
>
    {% block content %}{% endblock %}
</table>
