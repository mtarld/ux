{%- props -%}
{%- set style = html_cva(
    base: '[&_tr:last-child]:border-0',
    variants: {},
    compoundVariants: []
) -%}

<tbody
        class="{{ style.apply({}, attributes.render('class'))|tailwind_merge }}"
        {{ attributes.defaults({}).without('class') }}
>
    {% block content %}{% endblock %}
</tbody>
