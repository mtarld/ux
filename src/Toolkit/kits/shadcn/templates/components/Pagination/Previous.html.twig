<twig:Pagination:Link
    aria-label="Go to previous page"
    size="default"
    class="{{ 'gap-1 pl-2.5 ' ~ attributes.render('class')|tailwind_merge }}"
    {{ ...attributes.without('class') }}
>
    <twig:UX:Icon name="lucide:chevron-left" class="size-4" />
    <span>Previous</span>
</twig:Pagination:Link>
